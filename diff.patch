+     print('1D array: ', a)
+     print('shape of a: ', a.shape())
+     
+     a.transpose()
+     print('\ntranspose of array: ', a)
+     print('shape of a: ', a.shape())
+     
+     
+     a = ndarray([[1, 2, 3, 4], [5, 6, 7, 8]])
+     print('\n2D array: \n', a)
+     print('shape of a: ', a.shape())
+     
+     a.transpose()
+     print('\ntranspose of array: \n', a)
+     print('shape of a: ', a.shape())
