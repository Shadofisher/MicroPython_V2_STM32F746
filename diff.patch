+         node->child = NULL;
+         node->next = NULL;
+         node = mp_pairheap_pairing(lt, child);
+         parent->child = node;
+     } else {
+         mp_pairheap_t *n = parent->child;
+         while (node != n->next) {
+             n = n->next;
+         }
+         mp_pairheap_t *child = node->child;
+         next = node->next;
+         node->child = NULL;
+         node->next = NULL;
+         node = mp_pairheap_pairing(lt, child);
