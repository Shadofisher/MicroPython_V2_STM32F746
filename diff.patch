+             int top_n = MIN(len_c, len_a - k);
+             int bot_n = MAX(-k, 0);
+             mp_float_t* a_ptr = a_items + bot_n + k;
+             mp_float_t* a_end = a_ptr + (top_n - bot_n);
+             mp_float_t* c_ptr = c_items + len_c - bot_n - 1;
