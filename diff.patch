+ #ifdef CIRCUITPY
+ mp_obj_t ndarray_make_new(const mp_obj_type_t *type, size_t n_args, const mp_obj_t *args, mp_map_t *kw_args) {
+     (void) type;
+     mp_arg_check_num(n_args, kw_args, 1, 2, true);
+     size_t n_kw = 0;
+     if (kw_args != 0) {
+         n_kw = kw_args->used;
+     }
+     mp_map_init_fixed_table(kw_args, n_kw, args + n_args);
