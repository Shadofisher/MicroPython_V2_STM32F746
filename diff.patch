+ static void I2Cx_Error(I2C_HandleTypeDef *i2c_handler, uint8_t Addr)
+ {
+   /* De-initialize the I2C communication bus */
+   HAL_I2C_DeInit(i2c_handler);
+ 
+   /* Re-Initialize the I2C communication bus */
+   I2Cx_Init(i2c_handler);
+ }
