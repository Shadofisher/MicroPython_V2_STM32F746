+         return self._get()
+ 
+     def _put(self, val):
+         self._queue.append(val)
+ 
+     def put(self, val):
+         """Returns generator which can be used for putting item in a queue.
+ 
+         Usage::
+ 
+             yield from queue.put(item)
+         """
+         while self.qsize() >= self.maxsize and self.maxsize:
+             yield from sleep(self._attempt_delay)
